.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/PixelMath-pxm.dylib $(PCLBINDIR64)
	codesign --deep --options=runtime -s pleiades -f -v --timestamp $(PCLBINDIR64)/PixelMath-pxm.dylib

