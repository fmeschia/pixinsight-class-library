	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/PixelMath-pxm.so

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/PixelMath-pxm.so $(PCLBINDIR64C)

