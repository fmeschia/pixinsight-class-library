#

SRC_FILES= \
../../SandboxInstance.cpp \
../../SandboxInterface.cpp \
../../SandboxModule.cpp \
../../SandboxParameters.cpp \
../../SandboxProcess.cpp

#
# Object files
#

OBJ_FILES= \
./x64/Release/SandboxInstance.o \
./x64/Release/SandboxInterface.o \
./x64/Release/SandboxModule.o \
./x64/Release/SandboxParameters.o \
./x64/Release/SandboxProcess.o

#
# Dependency files
#

DEP_FILES= \
./x64/Release/SandboxInstance.d \
./x64/Release/SandboxInterface.d \
./x64/Release/SandboxModule.d \
./x64/Release/SandboxParameters.d \
./x64/Release/SandboxProcess.d

#
# Rules
#

-include $(DEP_FILES)

$(OBJ_DIR)/Sandbox-pxm.so: $(OBJ_FILES)
	g++ -m64 -fPIC -pthread -Wl,-fuse-ld=gold -Wl,--enable-new-dtags -Wl,-z,noexecstack -Wl,-O1 -Wl,--gc-sections -rdynamic -shared -L"$(PCLLIBDIR64C)" -L"$(PCLBINDIR64C)/lib" -o $(OBJ_DIR)/Sandbox-pxm.so $(OBJ_FILES) -lpthread -lPCL-pxi -llz4-pxi -lzlib-pxi -lRFC6234-pxi -llcms-pxi -lcminpack-pxi
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/Sandbox-pxm.so

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/Sandbox-pxm.so $(PCLBINDIR64C)

./x64/Release/%.o: ../../%.cpp
	g++ -c -pipe -pthread -m64 -fPIC -D_REENTRANT -D__PCL_LINUX -I"$(PCLINCDIR)" -I"$(PCLSRCDIR)/3rdparty" -mfpmath=sse -mssse3 -minline-all-stringops -O3 -fomit-frame-pointer -ffunction-sections -fdata-sections -ffast-math -fvisibility=hidden -fvisibility-inlines-hidden -fnon-call-exceptions -std=c++17 -Wall -Wno-parentheses -MMD -MP -MF"$(@:%.o=%.d)" -o"$@" "$<"
	@echo ' '

